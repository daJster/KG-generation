@prefix ns1: <http://example.org/> .

ns1:Gene ns1:encodes ns1:DNA .

ns1:Neurotransmitter ns1:subclass_of ns1:Communication .

ns1:Axon ns1:connects_with ns1:Dendrite ;
    ns1:part_of ns1:Neuron .

ns1:Chromosome ns1:has_part ns1:DNA .

ns1:Dendrite ns1:connects_with ns1:Axon ;
    ns1:part_of ns1:Neuron .

ns1:Messenger_RNA ns1:has_part ns1:DNA .

ns1:Myelin ns1:has_part ns1:Lipid .

ns1:Nervous_system ns1:has_part ns1:Neuron .

ns1:Synapse ns1:part_of ns1:Neuron .

ns1:DNA ns1:part_of ns1:Chromosome,
        ns1:Messenger_RNA .

ns1:Neuron ns1:has_part ns1:Myelin,
        ns1:Synapse ;
    ns1:part_of ns1:Nervous_system .

